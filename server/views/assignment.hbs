<div class = "display">
  <div class = "map" id = "assignmentMap">
  </div>
  <div class = "assignment">
    <div class = "barInfo">
      <div class = "barPic">
        <img id = "photo" src = "{{imageUrl}}" />
      </div>
      <div class = "barName">
        <p>{{name}}</p>
      </div>
      <div class = "barAddress">
        <p>124 Main St. Chicago, IL 60605</p>
      </div>
      <div class = "barPhone">
        <p>{{phone}}<p>
      </div>
    </div>
    <div id = "checkIn">
      <button class = "button">Check In</button>
    </div>
  </div>
</div>
